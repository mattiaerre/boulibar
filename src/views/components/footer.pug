.section
  .container
    .columns.has-text-centered
      .column
        img(alt="logo light" height="46" src="images/logo-light.svg" width="162")
    .columns
      .column
      each key in ['quote', 'order', 'payment', 'contact']
        .column.has-text-centered
          a(href=`/${key}`)= copy[key]
      .column
    .columns
      .column
      .column.has-text-centered
        ul.social
          li
            a(href="https://www.linkedin.com/company/moving-on-productions-llc" rel="noopener noreferrer" target="_blank")
              span.icon.is-medium
                i.fab.fa-linkedin-in
          li
            a(href="https://twitter.com/MovingOnProd" rel="noopener noreferrer" target="_blank")
              span.icon.is-medium
                i.fab.fa-twitter
          li
            a(href="https://www.facebook.com/movingontranscription" rel="noopener noreferrer" target="_blank")
              span.icon.is-medium
                i.fab.fa-facebook-f
      .column
