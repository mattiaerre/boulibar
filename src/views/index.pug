extends layout

block content
  if page.sections.includes('quote')
    include cta/quote
  if page.sections.includes('upload')
    include cta/upload
  .section
    .container
      .content
        h1= copy[page.key]
        blockquote.has-text-black= page.info
  .section
    .container
      .content
        case page.key
          when 'contact'
          when 'quote'
            #root
            script(src="javascripts/main.js")
            // script(src="http://localhost:3000/static/js/bundle.js")
          when 'payment'
            include paypal